<div id="channel-engagement-here"><%- include('images/slices/channel-engagement-blank.svg') %></div>
<script>
	const addChannelEngagementSVG = (data) => (`<%- include('images/slices/channel-engagement.svg') %>`);

	let triggeredChannelEngagementGraphAnimation = false;
	const animateChannelEngagementGraph = () => {
		if (window.scrollY > 700 && !triggeredChannelEngagementGraphAnimation) {
			triggeredChannelEngagementGraphAnimation = true;

			document.querySelector('#channel-engagement-here').innerHTML = addChannelEngagementSVG();
		}
	};

	document.addEventListener("scroll", animateChannelEngagementGraph);
</script>