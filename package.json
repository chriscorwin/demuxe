{
  "name": "demuxe",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.9.4"
  },
  "scripts": {
    "start": "./config/prestart.sh && node ./bin/www",
    "start:local:debug": "./config/prestart.sh && DEBUG=true NODE_ENV=local node ./bin/www",
    "start:local": "./config/prestart.sh && NODE_ENV=local node ./bin/www",
    "start:dev": "./config/prestart.sh && DEBUG=true NODE_ENV=dev node ./bin/www",
    "start:qa": "./config/prestart.sh && NODE_ENV=qa node ./bin/www",
    "test": "NODE_ENV=local mocha",
    "livereload": "livereload . -w 200 --extraExts 'scss,ejs,png,svg,js' -d",
    "test:update": "rm -rf test/screenshots/__different_snapshot__/ && NODE_ENV=local update=true mocha",
    "slides": "node ./slides/upload.js",
    "set-config": "./config/config.sh",
    "clean-svgs": "./config/clean-svgs.sh"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "git+https://github.com/cmcculloh/ejs.git",
    "express": "~4.16.0",
    "express-sanitizer": "^1.0.4",
    "express-session": "^1.16.2",
    "http-errors": "~1.6.2",
    "image-size": "^0.6.3",
    "livereload": "^0.8.0",
    "morgan": "~1.9.0",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "md2gslides": "^0.5.0",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "sf-differencify": "git+https://github.com/cormacmccarthy/sf-differencify.git",
    "svgo": "^1.3.0"
  }
}
