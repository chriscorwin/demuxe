<!DOCTYPE html>
<html>
	<head>
		<%- include('includes/global/head') %>
	</head>
	<% 
		let siteSectionClassname = '';
		if (locals.siteSection) { 
			siteSectionClassname += locals.siteSection;
		} else {
			siteSectionClassname += ' your-mom-wears-army-boots ';

		}

		locals.localization.body_class = ' ';
	%>


	<body class="<%= locals.localization.body_class %> <%= siteSectionClassname %> <%= locals.sanitizedQueryParams.debug==='true' ? 'has-debug' : '' %>">
		<div id="content-wrapper" class="slds-m-around_large slds-p-bottom_large">
			<div id="content" class="slds-container_fluid ">
				<script>console.log('<%= locals.root %>');</script>
				<%- include(`${locals.pageContents}`) %>
			</div>
		</div>

		<%- include('includes/global/footer-scripts') %>
		<%- include('includes/rpd/controller.ejs') %>
	</body>
</html>
